{
  "name": "onenuhs",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "env-cmd -e common,dev react-scripts start",
    "test": "env-cmd -e common,dev react-scripts test",
    "test:coverage": "env-cmd -e common,sit react-scripts test --watchAll=false --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "cross-env prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md,yml}'"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.9",
    "@reduxjs/toolkit": "^1.3.5",
    "@types/react-redux": "^7.1.15",
    "env-cmd": "^10.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@testing-library/react": "^9.5.0",
    "@types/enzyme": "^3.10.5",
    "@types/jest": "^25.2.2",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-mock-store": "^1.0.2",
    "babel-plugin-macros": "^2.8.0",
    "cross-env": "^7.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest-junit": "^10.0.0",
    "lint-staged": "^10.2.0",
    "prettier": "^2.0.4",
    "react-scripts": "3.4.1",
    "redux-mock-store": "^1.5.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "reporters": [
    "default",
    "jest-junit"
  ],
  "jest": {
    "coverageReporters": [
      "cobertura",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.tsx",
      "!src/**/setupTests.{js,jsx,ts,tsx}",
      "!src/**/serviceWorker.{js,jsx,ts,tsx}",
      "!src/**/*.(spec|test|stories).{js,jsx,ts,tsx}"
    ]
  },
  "jest-junit": {
    "reporters": [
      "jest-junit"
    ],
    "output": "./junit.xml",
    "classNameTemplate": "{classname}-{title}",
    "titleTemplate": "{classname}-{title}",
    "ancestorSeparator": " â€º ",
    "suiteNameTemplate": "{filepath}",
    "usePathForSuiteName": "true"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,scss,yml}": [
      "npm run lint -- --fix --max-warnings=0",
      "npm run format"
    ]
  }
}
